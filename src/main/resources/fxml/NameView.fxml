                     <font>
                        <Font size="8.0" />
                     </font>
                  </Text>
                  <ChoiceBox fx:id="musicDropdown" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="33.0" prefWidth="150.0" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="2" />
